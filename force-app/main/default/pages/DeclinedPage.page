<apex:page standardController="Case" tabstyle="case">
    <apex:form >
        <apex:pageBlock title="Case Edit">
            <apex:pageBlockButtons >
                <apex:commandButton action="{!save}" value="Save"/>
            </apex:pageBlockButtons>
            <apex:pageBlockSection title="Case Information">
                <apex:inputField value="{!case.CaseNumber}"/>
                <apex:inputField value="{!case.ContactId}"/>
                <apex:inputField value="{!case.AccountId}"/>
                <apex:inputField value="{!case.OwnerId}"/>
                <apex:inputField value="{!case.Type}"/>
                <apex:inputField value="{!case.Origin}"/>
                <apex:inputField value="{!case.Reason}"/>
                <apex:inputField value="{!case.Priority}"/>
                <apex:inputField value="{!case.Status}"/>
            </apex:pageBlockSection>
        </apex:pageBlock>
    </apex:form>
</apex:page>